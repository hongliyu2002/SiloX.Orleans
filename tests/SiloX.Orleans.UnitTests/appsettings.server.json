            "AdvertisedIPAddress": "127.0.0.1",
            "SiloPort": 11111,
            "GatewayPort": 30000,
            "SiloName": "Test-Silo"
        },
        "Clustering": {
            "UsedByClient": false,
            "ServiceId": "Test-Service",
            "ClusterId": "Test-Cluster",
            "InMemory": {
                "LocalServiceId": "Test-Service",
                "LocalClusterId": "Test-Cluster",
                "LocalSiloPort": 11111,
                "LocalGatewayPort": 30000
            },
            "Redis": {
                "ProviderName": "Redis-Cluster"
            },
            "AdoNet": {
